#include <stdio.h>
#include <conio.h>
void main()
{
int [20],i,j,key;
clrscr();
printf("Enter Number of ELemnts:");
scanf("%d",&n);
printf("Enter Elements:");
for(i=0;i<n;i++)
scanf("%d",&a[i]);

for(i=0;i<n;i++)
{
key=a[i];
j=i-1;
while(j>=0 && a[j]>key)
{
a[j+1]=a[j];
j--;
}
a[j+i]=key;
}
printf("Sorted array:");
for(i=0;i<n;i++)
{
printf("%d",a[i]);
}
getch();
}                                                    
