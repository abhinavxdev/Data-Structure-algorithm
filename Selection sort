#include <stdio.h>
#include <conio.h>
int main()
{
int a[20],n,i,j,t;
clrscr();
printf("Enter Number of ELements:");
scanf("%d",&n);

printf("Enter Elements:");
for(i=0;i<n;i++)
scanf("%d",&a[i]);

for(i=0;i=n-1;i++)
for(j=i+1;j<n;j++)
if(a[i]>a[j])
{
t=a[i];
a[i]=a[j];
a[j]=t;
}
for(i=0;i<n;i++)
printf("%d",a[i]);
getch();
}
